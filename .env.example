MEMCACHED_HOST=127.0.0.1

REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

RATE_LIMITER_ACTIVE=true
RATE_LIMITER_ENGINE=provider
RATE_LIMITER_ATTEMPTS_PER_HOUR=10
